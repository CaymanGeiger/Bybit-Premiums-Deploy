{
    "version": 2,
    "builds": [
        {
            "src": "src/app/next.config.js",
            "use": "@vercel/next"
        },
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/app/$1"
        }
    ]
}
