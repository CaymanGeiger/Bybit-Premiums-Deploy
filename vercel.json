{
    "version": 2,
    "functions": {
        "src/app/api/**": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        },
        {
            "src": "src/app/**",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
